                        {
                            "type": "text",
                            "name": "YrsOfKilns",
                            "visible": false,
                            "visibleIf": "{ExperienceArea} contains \"Kilns\"",
                            "title": "Kilns: Number of Years"
                        },
                        {
                            "type": "checkbox",
                            "name": "DrivingExp",
                            "title": "Driving training completed",
                            "isRequired": true,
                            "choices": [
                                {
                                    "value": "Class5",
                                    "text": "Class 5"
