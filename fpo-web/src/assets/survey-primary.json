                                }
                            ],
                            "colCount": 3
                        },
                        {
                            "type": "text",
                            "name": "OtherSawmillTraining",
                            "visible": false,
                            "visibleIf": "{SawmillTrained} contains \"Other\"",
                            "title": "Other training:",
                            "isRequired": true
